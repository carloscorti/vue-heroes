{
  "name": "vue-heroes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prebackend": "node ./createMockDb.js",
    "backend": "json-server --watch db.json --routes routes.json --port 8888 --middlewares ./node_modules/json-server-reset",
    "dev": "concurrently \"npm run backend\" \"npm run serve\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.21.0",
    "bulma": "^0.7.5",
    "core-js": "^2.6.5",
    "date-fns": "^2.16.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli": "^4.5.9",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.16.3",
    "json-server-reset": "^1.3.0",
    "node-sass": "^4.9.0",
